{
  "name": "github-localtime",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "add localtime to pages on github",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
        "jquery-1.9.1.min.js",
        "moment.min.js",
        "script.js"
      ]
    }
  ]
}
